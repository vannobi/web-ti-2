{
  "expo": {
    "name": "React Native Template Expo Kitten",
    "slug": "react-native-template-expo-kitten",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Template built using Expo, UI Kitten, React Native, React Native for Web & React Native Navigation.",
    "githubUrl": "https://github.com/elgsantos/react-native-template-expo-kitten"
  }
}
